{
    "name": "behnid",
    "version": "1.0.0",
    "description": "rest-api for behnid webapp",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "test": "jest --watch",
        "start": "node index.js",
        "restart": "pm2 restart expressjs",
        "dev": "nodemon index.js",
        "prettier": "npx prettier --write .",
        "re-deploy": "pm2 delete expressjs && pm2 start npm --name expressjs -i 2 -- run start -- -p 3001 ",
        "deploy": "pm2 start npm --name expressjs -i 2 -- run start -- -p 3001 ",
        "studio": "npx prisma studio",
        "migrate-dev": "npx prisma migrate dev",
        "gen_vapid_keys": " web-push generate-vapid-keys [--json]"
    },
    "keywords": [
        "behnid",
        "beh-trade",
        "behnid-app",
        "behnid-api"
    ],
    "author": "Behnid (beh-bazar)",
    "license": "ISC",
    "dependencies": {
        "@prisma/client": "4.6.1",
        "axios": "^1.4.0",
        "bcrypt": "^5.1.0",
        "body-parser": "^1.20.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "cron": "^2.2.0",
        "dotenv": "^16.0.1",
        "ejs": "^3.1.8",
        "express": "^4.18.1",
        "helmet": "^5.1.1",
        "jsonwebtoken": "^8.5.1",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "node-cron": "^3.0.2",
        "shelljs": "^0.8.5",
        "smsir-js": "^1.3.1",
        "socket.io": "^4.5.2",
        "web-push": "^3.5.0"
    },
    "devDependencies": {
        "jest": "^29.3.1",
        "kill-port": "^2.0.1",
        "nodemon": "^2.0.19",
        "prettier": "2.8.0",
        "prisma": "4.6.1",
        "supertest": "^6.3.1"
    }
